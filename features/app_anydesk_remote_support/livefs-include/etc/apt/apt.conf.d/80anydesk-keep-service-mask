DPkg::Pre-Install-Pkgs { "[ -e /etc/systemd/system/anydesk.service ] && grep -qs '/anydesk_' && echo 'anydesk about to be updated, preserving service mask status' && cp -av /etc/systemd/system/anydesk.service /tmp/etc--systemd--system--anydesk.service && cp -av /etc/systemd/system/anydesk.service /tmp/etc--systemd--system--anydesk.service.dupe || true" }
DPkg::Post-Invoke { "if [ -e /tmp/etc--systemd--system--anydesk.service ]; then echo 'restoring anydesk service status in /etc/systemd' && cp -av /etc/systemd/system/anydesk.service /tmp/etc--systemd--system--anydesk.service.new && mv -v /tmp/etc--systemd--system--anydesk.service /etc/systemd/system/anydesk.service; fi || true" }
